# Environment variables for Activity Tracker Server
PORT=3000

# Comma-separated list of allowed origins for CORS
# Example: http://localhost:8000,https://mydomain.com,https://research.university.edu
ALLOWED_ORIGINS=http://localhost:63342,https://proto.yux.digital,https://www.proto.yux.digital

# Database configuration (SQLite file will be created in data/ directory)
# No additional config needed for SQLite

# Optional: Add basic authentication for admin panel
# ADMIN_USERNAME=admin
# ADMIN_PASSWORD=your_secure_password_here

# Rate limiting (requests per minute)
RATE_LIMIT_TRACKING=100
RATE_LIMIT_DOWNLOADS=5
